{
  "name": "ponto-ilumeo-frontend",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/create-react-app"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://ponto-ilumeo-backend.vercel.app/api"
  }
} 